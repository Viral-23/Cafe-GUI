<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rucafe.project4.RUCafeMainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Pane prefHeight="382.0" prefWidth="360.0">
      <children>
          <Button layoutX="27.0" layoutY="31.0" onAction="#displayDonutsView">
            <graphic>
               <ImageView fitHeight="121.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@donut.jpeg" />
                  </image>
               </ImageView>
            </graphic></Button>
         <Button layoutX="195.0" layoutY="31.0" mnemonicParsing="false" onAction="#displayCoffeeView">
            <graphic>
               <ImageView fitHeight="121.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@coffee.jpeg" />
                  </image>
               </ImageView>
            </graphic></Button>
         <Button layoutX="26.0" layoutY="198.0" mnemonicParsing="false" onAction="#displayBasketView" prefHeight="130.0" prefWidth="147.0">
            <graphic>
               <ImageView fitHeight="127.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@cart.png" />
                  </image>
               </ImageView>
            </graphic></Button>
         <Button layoutX="195.0" layoutY="198.0" mnemonicParsing="false" onAction="#displayOrdersView">
            <graphic>
               <ImageView fitHeight="121.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@order.jpeg" />
                  </image>
               </ImageView>
            </graphic></Button>
      </children>
   </Pane>
</VBox>
